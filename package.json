{
	"name": "has-typed-arrays",
	"version": "0.0.0",
	"description": "Does this environment have JS Typed Arrays?",
	"main": "index.js",
	"exports": {
		".": [{
			".": "./index.js"
		}, "./index.js"]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"tests-only": "node test",
		"test": "npm run tests-only",
		"posttest": "npx aud",
		"version": "auto-changelog && git add CHANGELOG.md",
		"postversion": "auto-changelog && git add CHANGELOG.md && git commit --no-edit --amend && git tag -f \"v$(node -e \"console.log(require('./package.json').version)\")\""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/inspect-js/has-typed-arrays.git"
	},
	"keywords": [
		"typed array",
		"typed arrays",
		"TypedArrays",
		"js",
		"Float32Array",
		"Float64Array",
		"Int8Array",
		"Uint8Array",
		"Uint8ClampedArray",
		"Int16Array",
		"Uint16Array",
		"Int32Array",
		"Uint32Array",
		"BigInt64Array",
		"BigUint64Array"
	],
	"author": "Jordan Harband <ljharb@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/inspect-js/has-typed-arrays/issues"
	},
	"homepage": "https://github.com/inspect-js/has-typed-arrays#readme",
	"devDependencies": {
		"@ljharb/eslint-config": "^15.0.2",
		"auto-changelog": "^1.16.2",
		"eslint": "^6.7.2",
		"tape": "^4.11.0"
	},
	"funding": {
		"url": "https://github.com/sponsors/ljharb"
	},
	"dependencies": {
		"array.prototype.some": "^1.1.0",
		"which-typed-array": "^1.1.0"
	},
	"auto-changelog": {
